CREATE TABLE student_login (
    fname varchar(20) NOT null,
    lname varchar(20) NOT null,
    email varchar(20) NOT null,
    pwd varchar(20) NOT null
    );

 CREATE TABLE questions(
    question varchar(200),
    option1 varchar(20),
    option2 varchar(20),
    option3 varchar(20),
    option4 varchar(20)
    )